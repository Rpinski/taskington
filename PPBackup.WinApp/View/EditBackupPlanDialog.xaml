<dialogs:CustomDialog x:Class="PPBackup.WinApp.View.EditBackupPlanDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PPBackup.WinApp.View"
        xmlns:dialogs="clr-namespace:MahApps.Metro.Controls.Dialogs;assembly=MahApps.Metro" mc:Ignorable="d"
        Height="450" Width="800" Name="thisEditBackupPlanDialog">
    <Grid>
        <Button Name="closeButton" Command="{Binding CloseEditDialogCommand}"
                CommandParameter="{Binding ElementName=thisEditBackupPlanDialog}">Cancel</Button>
    </Grid>
</dialogs:CustomDialog>
